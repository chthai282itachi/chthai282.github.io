<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:a="urn:import:ch.unibas.medizin.osce.client.managed.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:ch.unibas.medizin.osce.client.scaffold.ui">
  <ui:style>
    .outer {
      padding: 10px;
    }
    .error {
      padding-left: 0.5em;
      background-color: red;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding-bottom: 10px;
    }
  </ui:style>
 
  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    <div class="{style.error}" ui:field="errors"/>
    
    
    
    
    
    
    

    <g:Button ui:field="save">Save</g:Button>
  
        
        
        
        
        
        
    <div id="item_defination">
            <div class="{style.label}">Item_defination</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="item_defination"/>
            </div>
        </div>
        <div id="item_name">
            <div class="{style.label}">Item_name</div>
            <div class="{style.value}">
                <g:TextBox ui:field="item_name"/>
            </div>
        </div>
        <div id="deleted">
            <div class="{style.label}">Deleted</div>
            <div class="{style.value}">
                <g:CheckBox ui:field="deleted"/>
            </div>
        </div>
        <div id="roleTemplate">
            <div class="{style.label}">Role Template</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="roleTemplate"/>
            </div>
        </div>
        <div id="sort_order">
            <div class="{style.label}">Sort_order</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="sort_order"/>
            </div>
        </div>
        <div id="roleSubItem">
            <div class="{style.label}">Role Sub Item</div>
            <div class="{style.value}">
                <a:RoleSubItemValueListEditor ui:field="roleSubItem"/>
            </div>
        </div>
        <div id="roleTableItem">
            <div class="{style.label}">Role Table Item</div>
            <div class="{style.value}">
                <a:RoleTableItemListEditor ui:field="roleTableItem"/>
            </div>
        </div>
        <div id="roleItemAccess">
            <div class="{style.label}">Role Item Access</div>
            <div class="{style.value}">
                <a:RoleItemAccessSetEditor ui:field="roleItemAccess"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
