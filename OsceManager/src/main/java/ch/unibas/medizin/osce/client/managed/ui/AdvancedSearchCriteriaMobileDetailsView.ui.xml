<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
    <div id="field">
            <div class="{style.label}">Field</div>
            <div class="{style.value}" ui:field="field"/>
        </div>
    <div id="objectId">
            <div class="{style.label}">Object Id</div>
            <div class="{style.value}" ui:field="objectId"/>
        </div>
    <div id="bindType">
            <div class="{style.label}">Bind Type</div>
            <div class="{style.value}" ui:field="bindType"/>
        </div>
    <div id="comparation">
            <div class="{style.label}">Comparation</div>
            <div class="{style.value}" ui:field="comparation"/>
        </div>
    <div id="value">
            <div class="{style.label}">Value</div>
            <div class="{style.value}" ui:field="value"/>
        </div>
    <div id="shownValue">
            <div class="{style.label}">Shown Value</div>
            <div class="{style.value}" ui:field="shownValue"/>
        </div>
    <div id="standardizedRole">
            <div class="{style.label}">Standardized Role</div>
            <div class="{style.value}" ui:field="standardizedRole"/>
        </div>

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
