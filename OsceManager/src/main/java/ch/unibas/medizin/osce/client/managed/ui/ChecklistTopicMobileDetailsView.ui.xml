<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName="{style.outer}">
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
    <div id="sort_order">
            <div class="{style.label}">Sort_order</div>
            <div class="{style.value}" ui:field="sort_order"/>
        </div>
    <div id="title">
            <div class="{style.label}">Title</div>
            <div class="{style.value}" ui:field="title"/>
        </div>
    <div id="checkList">
            <div class="{style.label}">Check List</div>
            <div class="{style.value}" ui:field="checkList"/>
        </div>
    <div id="checkListQuestions">
            <div class="{style.label}">Check List Questions</div>
            <div class="{style.value}" ui:field="checkListQuestions"/>
        </div>
    <div id="description">
            <div class="{style.label}">Description</div>
            <div class="{style.value}" ui:field="description"/>
        </div>

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
