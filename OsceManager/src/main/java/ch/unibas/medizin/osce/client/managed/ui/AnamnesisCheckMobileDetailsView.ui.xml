<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>
    <div id='text'><div class='{style.label}'>Text</div><div class='{style.value}' ui:field='text'></div></div>
    <div id='value'><div class='{style.label}'>Value</div><div class='{style.value}' ui:field='value'></div></div>
    <div id='sort_order'><div class='{style.label}'>Sort_order</div><div class='{style.value}' ui:field='sort_order'></div></div>
    <div id='type'><div class='{style.label}'>Type</div><div class='{style.value}' ui:field='type'></div></div>
    <div id='anamnesischecksvalues'><div class='{style.label}'>Anamnesischecksvalues</div><div class='{style.value}' ui:field='anamnesischecksvalues'></div></div>
    <div id='title'><div class='{style.label}'>Title</div><div class='{style.value}' ui:field='title'></div></div>
    <div id='userSpecifiedOrder'><div class='{style.label}'>User Specified Order</div><div class='{style.value}' ui:field='userSpecifiedOrder'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
