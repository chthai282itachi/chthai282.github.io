<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>
    <div id='roomNumber'><div class='{style.label}'>Room Number</div><div class='{style.value}' ui:field='roomNumber'></div></div>
    <div id='oscePostRooms'><div class='{style.label}'>Osce Post Rooms</div><div class='{style.value}' ui:field='oscePostRooms'></div></div>
    <div id='length'><div class='{style.label}'>Length</div><div class='{style.value}' ui:field='length'></div></div>
    <div id='width'><div class='{style.label}'>Width</div><div class='{style.value}' ui:field='width'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
