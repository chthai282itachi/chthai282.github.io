<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>
    <div id='shibId'><div class='{style.label}'>Shib Id</div><div class='{style.value}' ui:field='shibId'></div></div>
    <div id='logtime'><div class='{style.label}'>Logtime</div><div class='{style.value}' ui:field='logtime'></div></div>
    <div id='oldValue'><div class='{style.label}'>Old Value</div><div class='{style.value}' ui:field='oldValue'></div></div>
    <div id='newValue'><div class='{style.label}'>New Value</div><div class='{style.value}' ui:field='newValue'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
