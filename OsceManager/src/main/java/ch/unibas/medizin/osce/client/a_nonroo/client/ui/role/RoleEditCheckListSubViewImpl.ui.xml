<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:d='urn:import:com.google.gwt.user.datepicker.client'
  xmlns:r='urn:import:ch.unibas.medizin.osce.client.scaffold.ui'
  xmlns:a='urn:import:ch.unibas.medizin.osce.client.managed.ui'
  xmlns:my='urn:import:ch.unibas.medizin.osce.client.style.widgets'
  xmlns:b='urn:import:com.google.gwt.user.cellview.client'
  xmlns:n='urn:import:ch.unibas.medizin.osce.client.a_nonroo.client.ui.sp'>

<ui:style src="../../styles.css" >
.noOutline {
		outline-style: none;
		overflow: hidden;	
	}
</ui:style>
 
<g:HTMLPanel>
	<table class="{style.fields}">
	<tr id="shortname">						
		<td><g:Label styleName="{style.label}">Name</g:Label></td>
		<td><g:TextBox ui:field='title'/></td>
	</tr>
	</table>
</g:HTMLPanel>
</ui:UiBinder>
