<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .fields {
      margin-top: 0.5em;
      margin-left: 1em;
    }
    .label {
      min-height: 25px;
      font-weight: bold;
    }
    .button {
      margin-right: 1em;
    }
    .bar {
      margin-left: 1em;
    }
    .header {
      margin-left: 1em;
      color: #4B4A4A;
      text-shadow: #ddf 1px 1px 0;
      margin-bottom: 0;
    }
    .underline {
      border-bottom: 2px solid #6F7277;
    }
  </ui:style>
  <g:HTMLPanel>
    <div class="{style.underline}">
      <h3 class="{style.header}">
                <ui:msg>
                    <span ui:field="displayRenderer"/>
                </ui:msg>
            </h3>
    </div>

    <table class="{style.fields}" id="boundElementHolder">
      <tr id="gender">
                <td>
                    <span class="{style.label}">Gender:</span>
                </td>
                <td>
                    <span ui:field="gender"/>
                </td>
            </tr>
      <tr id="title">
                <td>
                    <span class="{style.label}">Title:</span>
                </td>
                <td>
                    <span ui:field="title"/>
                </td>
            </tr>
      <tr id="name">
                <td>
                    <span class="{style.label}">Name:</span>
                </td>
                <td>
                    <span ui:field="name"/>
                </td>
            </tr>
      <tr id="preName">
                <td>
                    <span class="{style.label}">Pre Name:</span>
                </td>
                <td>
                    <span ui:field="preName"/>
                </td>
            </tr>
      <tr id="email">
                <td>
                    <span class="{style.label}">Email:</span>
                </td>
                <td>
                    <span ui:field="email"/>
                </td>
            </tr>
      <tr id="telephone">
                <td>
                    <span class="{style.label}">Telephone:</span>
                </td>
                <td>
                    <span ui:field="telephone"/>
                </td>
            </tr>
      <tr id="clinic">
                <td>
                    <span class="{style.label}">Clinic:</span>
                </td>
                <td>
                    <span ui:field="clinic"/>
                </td>
            </tr>
      <tr id="office">
                <td>
                    <span class="{style.label}">Office:</span>
                </td>
                <td>
                    <span ui:field="office"/>
                </td>
            </tr>
      <tr id="isActive">
                <td>
                    <span class="{style.label}">Is Active:</span>
                </td>
                <td>
                    <span ui:field="isActive"/>
                </td>
            </tr>
      <tr id="specialisation">
                <td>
                    <span class="{style.label}">Specialisation:</span>
                </td>
                <td>
                    <span ui:field="specialisation"/>
                </td>
            </tr>
      <tr id="roleParticipants">
                <td>
                    <span class="{style.label}">Role Participants:</span>
                </td>
                <td>
                    <span ui:field="roleParticipants"/>
                </td>
            </tr>
      <tr id="id">
                <td>
                    <span class="{style.label}">Id:</span>
                </td>
                <td>
                    <span ui:field="id"/>
                </td>
            </tr>
      <tr id="version">
                <td>
                    <span class="{style.label}">Version:</span>
                </td>
                <td>
                    <span ui:field="version"/>
                </td>
            </tr>
    </table>

  <div class="{style.bar}">
    <g:Button addStyleNames="{style.button}" ui:field="edit">Edit</g:Button>
    <g:Anchor ui:field="delete">Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>
