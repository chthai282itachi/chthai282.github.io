<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='semester'><div class='{style.label}'>Semester</div><div class='{style.value}' ui:field='semester'></div></div>
    <div id='calYear'><div class='{style.label}'>Cal Year</div><div class='{style.value}' ui:field='calYear'></div></div>
    <div id='administrators'><div class='{style.label}'>Administrators</div><div class='{style.value}' ui:field='administrators'></div></div>
    <div id='osces'><div class='{style.label}'>Osces</div><div class='{style.value}' ui:field='osces'></div></div>
    <div id='patientsInSemester'><div class='{style.label}'>Patients In Semester</div><div class='{style.value}' ui:field='patientsInSemester'></div></div>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
