<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .fields {
      margin-top: 0.5em;
      margin-left: 1em;
    }
    .label {
      min-height: 25px;
      font-weight: bold;
    }
    .button {
      margin-right: 1em;
    }
    .bar {
      margin-left: 1em;
    }
    .header {
      margin-left: 1em;
      color: #4B4A4A;
      text-shadow: #ddf 1px 1px 0;
      margin-bottom: 0;
    }
    .underline {
      border-bottom: 2px solid #6F7277;
    }
  </ui:style>
  <g:HTMLPanel>
    <div class="{style.underline}">
      <h3 class="{style.header}">
                <ui:msg>
                    <span ui:field="displayRenderer"/>
                </ui:msg>
            </h3>
    </div>

    <table class="{style.fields}" id="boundElementHolder">
      <tr id="shortName">
                <td>
                    <span class="{style.label}">Short Name:</span>
                </td>
                <td>
                    <span ui:field="shortName"/>
                </td>
            </tr>
      <tr id="longName">
                <td>
                    <span class="{style.label}">Long Name:</span>
                </td>
                <td>
                    <span ui:field="longName"/>
                </td>
            </tr>
      <tr id="caseDescription">
                <td>
                    <span class="{style.label}">Case Description:</span>
                </td>
                <td>
                    <span ui:field="caseDescription"/>
                </td>
            </tr>
      <tr id="roleScript">
                <td>
                    <span class="{style.label}">Role Script:</span>
                </td>
                <td>
                    <span ui:field="roleScript"/>
                </td>
            </tr>
      <tr id="roleType">
                <td>
                    <span class="{style.label}">Role Type:</span>
                </td>
                <td>
                    <span ui:field="roleType"/>
                </td>
            </tr>
      <tr id="active">
                <td>
                    <span class="{style.label}">Active:</span>
                </td>
                <td>
                    <span ui:field="active"/>
                </td>
            </tr>
      <tr id="roleTopic">
                <td>
                    <span class="{style.label}">Role Topic:</span>
                </td>
                <td>
                    <span ui:field="roleTopic"/>
                </td>
            </tr>
      <tr id="roleParticipants">
                <td>
                    <span class="{style.label}">Role Participants:</span>
                </td>
                <td>
                    <span ui:field="roleParticipants"/>
                </td>
            </tr>
      <tr id="previousVersion">
                <td>
                    <span class="{style.label}">Previous Version:</span>
                </td>
                <td>
                    <span ui:field="previousVersion"/>
                </td>
            </tr>
      <tr id="studyYear">
                <td>
                    <span class="{style.label}">Study Year:</span>
                </td>
                <td>
                    <span ui:field="studyYear"/>
                </td>
            </tr>
      <tr id="mainVersion">
                <td>
                    <span class="{style.label}">Main Version:</span>
                </td>
                <td>
                    <span ui:field="mainVersion"/>
                </td>
            </tr>
      <tr id="subVersion">
                <td>
                    <span class="{style.label}">Sub Version:</span>
                </td>
                <td>
                    <span ui:field="subVersion"/>
                </td>
            </tr>
      <tr id="keywords">
                <td>
                    <span class="{style.label}">Keywords:</span>
                </td>
                <td>
                    <span ui:field="keywords"/>
                </td>
            </tr>
      <tr id="advancedSearchCriteria">
                <td>
                    <span class="{style.label}">Advanced Search Criteria:</span>
                </td>
                <td>
                    <span ui:field="advancedSearchCriteria"/>
                </td>
            </tr>
      <tr id="simpleSearchCriteria">
                <td>
                    <span class="{style.label}">Simple Search Criteria:</span>
                </td>
                <td>
                    <span ui:field="simpleSearchCriteria"/>
                </td>
            </tr>
      <tr id="checkList">
                <td>
                    <span class="{style.label}">Check List:</span>
                </td>
                <td>
                    <span ui:field="checkList"/>
                </td>
            </tr>
      <tr id="id">
                <td>
                    <span class="{style.label}">Id:</span>
                </td>
                <td>
                    <span ui:field="id"/>
                </td>
            </tr>
      <tr id="version">
                <td>
                    <span class="{style.label}">Version:</span>
                </td>
                <td>
                    <span ui:field="version"/>
                </td>
            </tr>
    </table>

  <div class="{style.bar}">
    <g:Button addStyleNames="{style.button}" ui:field="edit">Edit</g:Button>
    <g:Anchor ui:field="delete">Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>
