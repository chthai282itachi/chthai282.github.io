<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style src="../styles.css" />

<g:HTMLPanel addStyleNames="{style.patientDetails}">
	<table id="boundElementHolder" class="{style.fields}">
	<colgroup>
		<col></col>
		<col></col>
	</colgroup>
	<tbody>
		<tr id="title">
			<td><span class="{style.label}" ui:field="labelTitle">Title:</span></td>
			<td><span ui:field="title"/></td>
		</tr>
		<tr id="name">
			<td><span class="{style.label}" ui:field="labelName">Name:</span></td>
	      	<td><span ui:field="name"/></td>
		</tr>
		<tr id="preName">
			<td><span class="{style.label}" ui:field="labelPreName">Pre Name:</span></td>
			<td><span ui:field="preName"/></td>
		</tr>
		<tr id="gender">
			<td><span class="{style.label}" ui:field="labelGender">Gender:</span></td>
			<td><span ui:field="gender"/></td>
		</tr>
		<tr id="email">
			<td><span class="{style.label}" ui:field="labelEmail">Email:</span></td>
			<td><g:Anchor ui:field="email"/></td>
		</tr>
		<tr id="telephone">
			<td><span class="{style.label}" ui:field="labelTelephone">Telephone:</span></td>
			<td><span ui:field="telephone"/></td>
		</tr>
	</tbody>
	</table>
</g:HTMLPanel>
</ui:UiBinder>
