<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='link'><div class='{style.label}'>Link</div><div class='{style.value}' ui:field='link'></div></div>
    <div id='comment'><div class='{style.label}'>Comment</div><div class='{style.value}' ui:field='comment'></div></div>
    <div id='standardizedPatient'><div class='{style.label}'>Standardized Patient</div><div class='{style.value}' ui:field='standardizedPatient'></div></div>
    <div id='contentType'><div class='{style.label}'>Content Type</div><div class='{style.value}' ui:field='contentType'></div></div>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
