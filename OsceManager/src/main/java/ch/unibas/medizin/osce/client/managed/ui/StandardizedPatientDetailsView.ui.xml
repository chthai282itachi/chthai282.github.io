            </tr>
      <tr id="anamnesisForm">
                <td>
                    <span class="{style.label}">Anamnesis Form:</span>
                </td>
                <td>
                    <span ui:field="anamnesisForm"/>
                </td>
            </tr>
      <tr id="langskills">
                <td>
                    <span class="{style.label}">Langskills:</span>
                </td>
                <td>
                    <span ui:field="langskills"/>
                </td>
            </tr>
    </table>

  <div class="{style.bar}">
    <g:Button addStyleNames="{style.button}" ui:field="edit">Edit</g:Button>
    <g:Anchor ui:field="delete">Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>
