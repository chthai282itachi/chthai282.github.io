<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .fields {
      margin-top: 0.5em;
      margin-left: 1em;
    }
    .label {
      min-height: 25px;
      font-weight: bold;
    }
    .button {
      margin-right: 1em;
    }
    .bar {
      margin-left: 1em;
    }
    .header {
      margin-left: 1em;
      color: #4B4A4A;
      text-shadow: #ddf 1px 1px 0;
      margin-bottom: 0;
    }
    .underline {
      border-bottom: 2px solid #6F7277;
    }
  </ui:style>
  <g:HTMLPanel>
    <div class="{style.underline}">
      <h3 class="{style.header}">
                <ui:msg>
                    <span ui:field="displayRenderer"/>
                </ui:msg>
            </h3>
    </div>

    <table class="{style.fields}" id="boundElementHolder">
      <tr id="id">
                <td>
                    <span class="{style.label}">Id:</span>
                </td>
                <td>
                    <span ui:field="id"/>
                </td>
            </tr>
      <tr id="version">
                <td>
                    <span class="{style.label}">Version:</span>
                </td>
                <td>
                    <span ui:field="version"/>
                </td>
            </tr>
      <tr id="gender">
                <td>
                    <span class="{style.label}">Gender:</span>
                </td>
                <td>
                    <span ui:field="gender"/>
                </td>
            </tr>
      <tr id="name">
                <td>
                    <span class="{style.label}">Name:</span>
                </td>
                <td>
                    <span ui:field="name"/>
                </td>
            </tr>
      <tr id="preName">
                <td>
                    <span class="{style.label}">Pre Name:</span>
                </td>
                <td>
                    <span ui:field="preName"/>
                </td>
            </tr>
      <tr id="street">
                <td>
                    <span class="{style.label}">Street:</span>
                </td>
                <td>
                    <span ui:field="street"/>
                </td>
            </tr>
      <tr id="city">
                <td>
                    <span class="{style.label}">City:</span>
                </td>
                <td>
                    <span ui:field="city"/>
                </td>
            </tr>
      <tr id="postalCode">
                <td>
                    <span class="{style.label}">Postal Code:</span>
                </td>
                <td>
                    <span ui:field="postalCode"/>
                </td>
            </tr>
      <tr id="telephone">
                <td>
                    <span class="{style.label}">Telephone:</span>
                </td>
                <td>
                    <span ui:field="telephone"/>
                </td>
            </tr>
      <tr id="telephone2">
                <td>
                    <span class="{style.label}">Telephone2:</span>
                </td>
                <td>
                    <span ui:field="telephone2"/>
                </td>
            </tr>
      <tr id="mobile">
                <td>
                    <span class="{style.label}">Mobile:</span>
                </td>
                <td>
                    <span ui:field="mobile"/>
                </td>
            </tr>
      <tr id="height">
                <td>
                    <span class="{style.label}">Height:</span>
                </td>
                <td>
                    <span ui:field="height"/>
                </td>
            </tr>
      <tr id="weight">
                <td>
                    <span class="{style.label}">Weight:</span>
                </td>
                <td>
                    <span ui:field="weight"/>
                </td>
            </tr>
      <tr id="immagePath">
                <td>
                    <span class="{style.label}">Immage Path:</span>
                </td>
                <td>
                    <span ui:field="immagePath"/>
                </td>
            </tr>
      <tr id="videoPath">
                <td>
                    <span class="{style.label}">Video Path:</span>
                </td>
                <td>
                    <span ui:field="videoPath"/>
                </td>
            </tr>
      <tr id="nationality">
                <td>
                    <span class="{style.label}">Nationality:</span>
                </td>
                <td>
                    <span ui:field="nationality"/>
                </td>
            </tr>
      <tr id="profession">
