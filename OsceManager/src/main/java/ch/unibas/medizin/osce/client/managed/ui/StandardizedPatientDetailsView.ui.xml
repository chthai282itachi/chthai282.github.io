                    <span class="{style.label}">Profession:</span>
                </td>
                <td>
                    <span ui:field="profession"/>
                </td>
            </tr>
      <tr id="birthday">
                <td>
                    <span class="{style.label}">Birthday:</span>
                </td>
                <td>
                    <span ui:field="birthday"/>
                </td>
            </tr>
      <tr id="email">
                <td>
                    <span class="{style.label}">Email:</span>
                </td>
                <td>
                    <span ui:field="email"/>
                </td>
            </tr>
      <tr id="descriptions">
                <td>
                    <span class="{style.label}">Descriptions:</span>
                </td>
                <td>
                    <span ui:field="descriptions"/>
                </td>
            </tr>
      <tr id="bankAccount">
                <td>
                    <span class="{style.label}">Bank Account:</span>
                </td>
                <td>
                    <span ui:field="bankAccount"/>
                </td>
            </tr>
      <tr id="maritalStatus">
                <td>
                    <span class="{style.label}">Marital Status:</span>
                </td>
                <td>
                    <span ui:field="maritalStatus"/>
                </td>
            </tr>
      <tr id="workPermission">
                <td>
                    <span class="{style.label}">Work Permission:</span>
                </td>
                <td>
