<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='studyYear'><div class='{style.label}'>Study Year</div><div class='{style.value}' ui:field='studyYear'></div></div>
    <div id='maxNumberStudents'><div class='{style.label}'>Max Number Students</div><div class='{style.value}' ui:field='maxNumberStudents'></div></div>
    <div id='numberPosts'><div class='{style.label}'>Number Posts</div><div class='{style.value}' ui:field='numberPosts'></div></div>
    <div id='numberCourses'><div class='{style.label}'>Number Courses</div><div class='{style.value}' ui:field='numberCourses'></div></div>
    <div id='postLength'><div class='{style.label}'>Post Length</div><div class='{style.value}' ui:field='postLength'></div></div>
    <div id='isRepeOsce'><div class='{style.label}'>Is Repe Osce</div><div class='{style.value}' ui:field='isRepeOsce'></div></div>
    <div id='numberRooms'><div class='{style.label}'>Number Rooms</div><div class='{style.value}' ui:field='numberRooms'></div></div>
    <div id='isValid'><div class='{style.label}'>Is Valid</div><div class='{style.value}' ui:field='isValid'></div></div>
    <div id='semester'><div class='{style.label}'>Semester</div><div class='{style.value}' ui:field='semester'></div></div>
    <div id='osce_days'><div class='{style.label}'>Osce_days</div><div class='{style.value}' ui:field='osce_days'></div></div>
    <div id='courses'><div class='{style.label}'>Courses</div><div class='{style.value}' ui:field='courses'></div></div>
    <div id='tasks'><div class='{style.label}'>Tasks</div><div class='{style.value}' ui:field='tasks'></div></div>
    <div id='osceStudents'><div class='{style.label}'>Osce Students</div><div class='{style.value}' ui:field='osceStudents'></div></div>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
