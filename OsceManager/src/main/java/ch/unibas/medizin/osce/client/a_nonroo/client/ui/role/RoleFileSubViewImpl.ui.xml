<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.google.gwt.user.cellview.client"
	xmlns:my="urn:import:ch.unibas.medizin.osce.client.style.widgets">
	<ui:style src="../../styles.css" />

	<g:HTMLPanel>
		
			<b:CellTable ui:field="table" addStyleNames="{style.standardTable}" />
	<g:HorizontalPanel>
			<g:FormPanel ui:field="uploadFormPanel">
				<g:HorizontalPanel>
					<g:FileUpload ui:field="fileUpload" name="fileUpload1"></g:FileUpload>
				</g:HorizontalPanel>
			</g:FormPanel>
			
				<g:TextBox ui:field="fileDescription"></g:TextBox>
				<my:IconButton ui:field="newButton" icon="plusthick"></my:IconButton>
			</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 