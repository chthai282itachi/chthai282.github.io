<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:n="urn:import:ch.unibas.medizin.osce.client.a_nonroo.client.ui.role"
	xmlns:b="urn:import:com.google.gwt.user.cellview.client"
	xmlns:my="urn:import:ch.unibas.medizin.osce.client.style.widgets" xmlns:d="urn:import:com.google.gwt.dom.client">
	<ui:style src="../../styles.css">
	.description {	}
	
	.padleft5
	{
		padding-left: 5px;
	}
	.icon
	{
		float: left;
		margin: 4px;
	}
	</ui:style>
	<ui:with type="ch.unibas.medizin.osce.client.style.resources.UiIcons" field="uiIcons"></ui:with>
	<g:HTMLPanel>
		<h1></h1>
		
		<g:VerticalPanel ui:field="TableItem" addStyleNames="{style.tabStyle}" spacing="10">
		<!-- n:RoleBaseTableItemViewImpl ui:field="roleBaseTableItemViewImpl"></n:RoleBaseTableItemViewImpl-->	 	  
		
		</g:VerticalPanel>
		<div class="{style.controls} {style.controlsleft}">
			
				<g:TextBox ui:field="ItemNameText" width="100"></g:TextBox>
				<g:ValueListBox ui:field="ItemtypeDD"></g:ValueListBox>
				<my:IconButton ui:field="AddItem" icon="plusthick"></my:IconButton>
		</div>
		<b:CellTable ui:field="table" addStyleNames="{style.standardTable}"/>
	</g:HTMLPanel>
	
</ui:UiBinder>
