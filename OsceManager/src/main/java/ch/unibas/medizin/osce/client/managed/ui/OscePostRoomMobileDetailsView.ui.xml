<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel id="boundElementHolder" styleName='{style.outer}'>
    <div id='room'><div class='{style.label}'>Room</div><div class='{style.value}' ui:field='room'></div></div>
    <div id='oscePost'><div class='{style.label}'>Osce Post</div><div class='{style.value}' ui:field='oscePost'></div></div>
    <div id='course'><div class='{style.label}'>Course</div><div class='{style.value}' ui:field='course'></div></div>
    <div id='assignments'><div class='{style.label}'>Assignments</div><div class='{style.value}' ui:field='assignments'></div></div>
    <div id='id'><div class='{style.label}'>Id</div><div class='{style.value}' ui:field='id'></div></div>
    <div id='version'><div class='{style.label}'>Version</div><div class='{style.value}' ui:field='version'></div></div>

    <g:Button ui:field='delete'>Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
